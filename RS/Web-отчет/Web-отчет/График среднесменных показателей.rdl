  <DataSets>
    <DataSet Name="DataSet_Отчет">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Параметры">
            <Value>=Parameters!Параметры.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        tbParams.NameParameter, tbValuesAVGSmenaDC12.NPech, tbValuesAVGSmenaDC12.Value, tbValuesAVGSmenaDC12.KodParameter
FROM            tbParams INNER JOIN
                         tbValuesAVGSmenaDC12 ON tbParams.KodParameter = tbValuesAVGSmenaDC12.KodParameter
WHERE        (tbValuesAVGSmenaDC12.KodParameter IN (@Параметры))</CommandText>
      </Query>
      <Fields>
        <Field Name="NameParameter">
          <DataField>NameParameter</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NPech">
          <DataField>NPech</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Value">
