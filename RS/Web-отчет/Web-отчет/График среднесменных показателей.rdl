        </Field>
        <Field Name="KodParameter">
          <DataField>KodParameter</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet_Параметры">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT KodParameter, NameParameter
FROM            tbParams
ORDER BY NameParameter</CommandText>
      </Query>
